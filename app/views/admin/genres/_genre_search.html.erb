<!--ユーザー用のジャンル一覧-->
<div class="card mb-4">
  <div id="headingOne">
    <h5 class="mb-0">
      <button class="btn btn-secondary w-100 text-white" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
        ジャンル検索一覧
      </button>
    </h5>
  </div>
  <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
    <div class="card-body">
      <tbody>
      <% @genres.each do |genre| %>
        <tr>
          <td>
            <%= link_to infos_path(genre_id: genre.id) , class: 'nav-link text-dark' do %>
              <%= genre.name %>
            <% end %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </div>
  </div>
</div>



