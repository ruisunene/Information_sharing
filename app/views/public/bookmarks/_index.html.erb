<!--ブックマーク一覧部分テンプレート-->
<div class="card mb-4">
  <div id="headingTwo">
    <h5 class="mb-0">
      <button class="btn btn-dark w-100 text-white" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
        ブックマーク一覧
      </button>
    </h5>
  </div>
  <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
    <div class="card-body">
      <tbody>
      <% @bookmarks.reverse_each do |bookmark| %>
        <tr>
          <td>
            <%= link_to info_path(bookmark.info), class: 'nav-link text-dark' do %>
              <%= bookmark.info.title %>
            <% end %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </div>
  </div>
</div>